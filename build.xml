<?xml version="1.0" encoding="UTF-8"?>
<project name="libnp" default="compile">

    <path id="libs">
      <fileset dir="lib/">
        <include name="*.jar"/>
      </fileset>
    </path>

    <target name="compile" description="Compiles libnp">
        <javac destdir="build/" srcdir="src/">
          <classpath refid="libs"/>
        </javac>
    </target>

</project>
